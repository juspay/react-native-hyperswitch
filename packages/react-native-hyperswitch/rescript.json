{
  "name": "@juspay-tech/react-native-hyperswitch",
  "namespace": true,
  "jsx": {"version": 4},
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "bsc-flags": ["-open RescriptCore"],
  "suffix": ".res.js",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": [
    "@rescript/react",
    "rescript-react-native",
    "@rescript/core"
  ],
  "uncurried": true,
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "importPath": "relative",
    "shims": {
      "Js": "Js"
    }
  }
}
